<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %} {{ encore_entry_link_tags('app') }} {% endblock %}
    </head>
    <body>
        {% block body %}
            <header>
                {% block header %} {% include '_navbar.html.twig' %} {% endblock %}
            </header>
            <main>
                {% block main %}{% endblock %}
            </main>
            <footer>
                {% block footer %}{% endblock %}
            </footer>
        {% endblock %}
        {% block javascripts %} {{ encore_entry_script_tags('app') }} {% endblock %}
    </body>
</html>
